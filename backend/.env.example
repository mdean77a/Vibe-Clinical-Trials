# Qdrant Configuration
# For Qdrant Cloud (recommended for production)
QDRANT_URL=https://your-cluster-url.qdrant.tech:6333
QDRANT_API_KEY=your-api-key-here

# For local Docker Qdrant (development)
# QDRANT_URL=http://localhost:6333
# QDRANT_API_KEY=  # Leave empty for local Docker

# For in-memory testing (not recommended for production)
# QDRANT_URL=  # Leave empty to use in-memory
# QDRANT_API_KEY=

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_EMBEDDING_MODEL=text-embedding-ada-002
OPENAI_MAX_RETRIES=3
OPENAI_TIMEOUT=30