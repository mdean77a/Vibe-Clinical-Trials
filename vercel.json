{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/protocols.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/health.py", 
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/protocols/(.*)",
      "dest": "/api/protocols.py"
    },
    {
      "src": "/api/(health|$)",
      "dest": "/api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "buildCommand": "cd frontend && npm run build",
  "functions": {
    "api/protocols.py": {
      "runtime": "python3.11"
    },
    "api/health.py": {
      "runtime": "python3.11"
    }
  },
  "env": {
    "PYTHONPATH": "./backend"
  }
} 